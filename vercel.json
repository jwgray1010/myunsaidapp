{
  "version": 2,
  "functions": {
    "vercel-api/v1/*.ts": {
      "runtime": "@vercel/node@3.0.21",
      "includeFiles": "vercel-api/**/*",
      "excludeFiles": "node_modules/**/*.map,**/test*/**,**/*.test.*,**/*.spec.*"
    }
  },
  "routes": [
    { "src": "^/api/v1/(.*)$", "dest": "/vercel-api/v1/$1" },
    { "src": "^/(.*)$", "dest": "/vercel-api/v1/health" }
  ]
}