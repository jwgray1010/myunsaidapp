  "version": "1.2.0",
  "notes": "Fallback-aware, additive deltas to base weights inside toneAnalysis.ts. Exact → alias → family → general → code_default. REQUIRES: resolveContextKey() integration in TS for alias/family mapping to work.",
  "mergeStrategy": "additive_deltas",
  "implementation": {
    "aliasResolution": "REQUIRES TS integration - aliasMap won't work without resolveContextKey() adapter",
    "familyResolution": "REQUIRES TS integration - CTX_* → family mapping needs resolver",
    "envKillSwitch": "REQUIRES TS integration - env check not implemented in current toneAnalysis.ts",
    "suggestionBounds": "NOT USED - minSuggestionWeight/maxSuggestionWeight not consumed by toneAnalysis.ts"
  },
  "bounds": { 
    "min": 0.0, 
    "max": 1.0,
    "note": "min/max are used by TS for weight clamping",
    "minSuggestionWeight": -2.0,
    "maxSuggestionWeight": 3.0,
    "suggestionNote": "NOT CONSUMED by toneAnalysis.ts - these bounds are ignored"
  },

  "baseWeights": {
    "emo": 0.40,
    "ctx": 0.20,
    "attach": 0.15,
    "ling": 0.15,
    "intensity": 0.10,
    "negPenalty": 0.15,
    "sarcPenalty": 0.18,
    "absolutesBoost": 0.06
  },

  "fallbacks": {
    "order": ["exact", "alias", "family", "general", "code_default"],
    "telemetryEvent": "weights.fallback",
    "enabled": true,
    "envKillSwitch": "DISABLE_WEIGHT_FALLBACKS",
    "note": "envKillSwitch requires TS integration - currently not checked by toneAnalysis.ts"
  },

